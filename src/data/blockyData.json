{
    "version": "1.0.0",
    "programName": "battery_collection_demo",
    "actions": [
      {
        "type": "repeat",
        "count": 3,
        "body": [
          { "type": "forward", "count": 1 }
        ]
      },
      {
        "type": "if",
        "cond": { 
          "type": "condition", 
          "function": "isGreen", 
          "check": true 
        },
        "then": [
          { "type": "collect", "color": "green", "count": 1 }
        ]
      }
    ]
}
