{
  "version": "1.0.0",
  "programName": "condition_demo",
  "actions": [
    {
      "type": "repeatRange",
      "variable": "i",
      "from": 1,
      "to": 5,
      "step": 1,
      "body": [
        { "type": "forward", "count": 1 },
        {
          "type": "if",
          "cond": {
            "type": "condition",
            "function": "isGreen",
            "check": true
          },
          "then": [
            { "type": "collect", "color": "green", "count": 3 }
          ],
          "elseIf": [
            {
              "cond": { "type": "condition", "function": "isRed", "check": true },
              "then": [ { "type": "collect", "color": "red", "count": 1 } ]
            }
         ]
        }
      ]
    }
  ]
}
