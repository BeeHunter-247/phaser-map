{
    "version": "1.0.0",
    "programName": "condition_demo",
    "actions": [
      {
        "type": "repeatRange",
        "variable": "i",
        "from": 1,
        "to": 3,
        "step": 1,
        "body": [
          { "type": "forward", "count": 1 },
          {
            "type": "if",
            "cond": {
              "type": "variableComparison",
              "variable": "i",
              "operator": "==",
              "value": 3
            },
            "then": [
              { "type": "collect", "color": "green", "count": 1 }
            ]
          }
        ]
      }
    ]
}
