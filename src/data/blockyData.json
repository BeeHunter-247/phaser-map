{
  "version": "1.0.0",
  "programName": "from_blockly_image",
  "actions": [
    {
      "type": "repeatRange",
      "variable": "i",
      "from": 1,
      "to": 3,
      "step": 1,
      "body": [
        { "type": "forward", "count": "{{i}}" },
        { "type": "collect", "color": "green", "count": { "type": "arithmetic", "op": "*", "left": "{{i}}", "right": 2 } }
      ]
    }
  ]
}
