{
  "version": "1.0.0",
  "programName": "from_blockly_image",
  "actions": [
    {
      "type": "repeat",
      "count": 4,
      "body": [
        { "type": "forward", "count": 1 },
        {
          "type": "if",
          "cond": {
            "type": "and",
            "conditions": [
              {
                "type": "variableComparison",
                "variable": "batteryCount",
                "operator": "==",
                "value": 3
              },
              {
                "type": "condition",
                "function": "isGreen",
                "check": true
              }
            ]
          },
          "then": [
            { "type": "collect", "color": "green", "count": 2 }
          ]
        }
      ]
    }
  ]
}
